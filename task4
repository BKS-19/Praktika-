package main

import "fmt"

func main() {
	var a, b int
	fmt.Scan(&a, &b)	
	fmt.Println(a + b)	
	fmt.Println(a - b)
	fmt.Println(a * b)
	if b != 0 {
		fmt.Println(a / b)
	} else {
		fmt.Println("ошибка")
	}
	if b != 0 {
		fmt.Println(a % b)
	} else {
		fmt.Println("ошибка")
	}
	if b != 0 {
		fmt.Printf("%.4f\n", float64(a)/float64(b))
	} else {
		fmt.Println("ошибка")
	}
}
